@model IEnumerable<SoftwareCelta.Models.dw_tipoDocumento>

@{
    ViewBag.Title = "Tipos Documentos";
}
<br /><br />
<h2>Tipos Documentos</h2>

<p>
    @Html.ActionLink("Crear Nueva", "Create", null, null, new { @class = "btn btn-primary" })
</p>
<div class="table-responsive">
    <table class="table">
        <tr>
            <th>
                Tipo Documento
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.nombreTipoDocumento)
                </td>
                <td>
                    @Html.ActionLink("Editar", "Edit", new { id = item.dw_tipoDocumentoID }) |
                    @Html.ActionLink("Eliminar", "Delete", new { id = item.dw_tipoDocumentoID })
                </td>
            </tr>
        }

    </table>
</div>