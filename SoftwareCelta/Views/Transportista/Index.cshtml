@model IEnumerable<SoftwareCelta.Models.dw_datosTransportista>

@{
    ViewBag.Title = "Index";
}

<h2>Transportistas</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create", null, null, new { @class = "btn btn-primary" })
</p>
<table class="table">
    <tr>
        <th>
            Rut
        </th>
        <th>
            Nombre Completo
        </th>
        <th>
            Razon Social
        </th>
        <th>
            Direccion
        </th>
        <th>
            Patente
        </th>      
        <th>
            Ciudad
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.rut)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombreCompleto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.razonSocial)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.patente)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ciudad)
        </td>       
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.dw_datosTransportistaID }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.dw_datosTransportistaID }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.dw_datosTransportistaID })
        </td>
    </tr>
}

</table>
