@model SoftwareCelta.Models.dw_envio

@{
    ViewBag.Title = "Details";
}

<h2>Detalle</h2>

<div>
    <h4>Datos Envio</h4>
    <hr />
    <dl class="dl-horizontal">       

        <dt>
            Vale Venta
        </dt>

        <dd>
            @Html.DisplayFor(model => model.valeVenta)
        </dd>

        <dt>
            Nombre de Cliente
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombreCliente)
        </dd>

        <dt>
            Rut Cliente
        </dt>

        <dd>
            @Html.DisplayFor(model => model.rutCliente)
        </dd>

        <dt>
            Ciudad
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ciudad)
        </dd>

        <dt>
            Telefono
        </dt>

        <dd>
            @Html.DisplayFor(model => model.telefono)
        </dd>

        <dt>
            Direccion
        </dt>

        <dd>
            @Html.DisplayFor(model => model.direccion)
        </dd>
       
        <dt>
            Observacion
        </dt>

        <dd>
            @Html.DisplayFor(model => model.observacion)
        </dd>
       
        <dt>
            Fecha de Venta
        </dt>

        <dd>
            @SoftwareCelta.Models.Formateador.fechaCompletaToString(@Model.fechaValeVenta)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.dw_envioID }) |
    @Html.ActionLink("Volver a la Lista", "Index")
</p>
